@model Aanmelding

@{
    ViewData["Title"] = "Aanmelden";
}

<section id="imageWrapper" class="mb-2">
    <img src="~/img/aanmeldBanner.png" alt="plaatje van orthopedagoog en client" class="rounded mx-auto d-block img-fluid">
</section>

<section id="signupWrapper">
    <div class="container">
        <div class="row">
            <h1>Gelukt</h1><br/>
            <p>Uw aanmelding is verzonden naar de orthopedagoog.</p>
        </div>
    </div>
</section>

<script>
    fetch("https://orthopedagogie-zmdh.herokuapp.com/Register?&Code=123", 
    {
        "method": "post",
         body: JSON.stringify({"geboortedatum": "", "BSN": ""}),
         "content-type": "application/json"
    })
    .then((i) => i.json())
    .then((i) => document.getElementById("BSN").innerText = i.BSN)
    .catch((err) => alert("api post call niet gelukt"));
</script>